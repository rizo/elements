(jbuild_version 1)

(library
 ((name Kernel)
  (public_name proto.kernel)
  (wrapped true)
  (libraries (proto.shadow-stdlib))
  (ocamlopt_flags (:standard -O3))
  (modules (Kernel))))

(library
 ((name Iter)
  (public_name proto.iter)
  (wrapped true)
  (ocamlopt_flags (:standard -O3))
  (modules (Iter))))

(library
 ((name Proto)
  (public_name proto)
  (wrapped false)
  (libraries (proto.kernel proto.iter proto.shadow-stdlib))
  (ocamlopt_flags (:standard -O3))
  (modules (Proto
            Proto_Data
            Proto_Debug
            Proto_Control
            Proto_Network
            Proto_System))))

